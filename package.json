{
    "name": "@cmmv/cli",
    "version": "0.10.13",
    "description": "CLI tool for CMMV applications",
    "type": "module",
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=16.14"
    },
    "bin": {
        "cmmv": "dist/bin/cmmv.js"
    },
    "keywords": [
        "cmmv",
        "ci",
        "npx",
        "start"
    ],
    "files": [
        "dist",
        "bin",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc",
        "clean": "find ./lib -type f \\( -name '*.d.ts' -o -name '*.js' \\) -exec rm -f {} +",
        "start": "node dist/bin/cmmv.js",
        "dev": "node dist/bin/cmmv dev --watch --debug",
        "release": "tsx release.ts",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "prepack": "npm run build",
        "prepublish:npm": "npm run build",
        "publish:npm": "npm publish --access public",
        "prerelease": "npm run build",
        "test": "vitest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cmmvio/cmmv-cli.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/cmmvio/cmmv-cli/issues"
    },
    "homepage": "https://github.com/cmmvio/cmmv-cli#readme",
    "dependencies": {
        "@cmmv/core": "latest",
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-angular": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.23.0",
        "@swc-node/core": "^1.13.3",
        "@swc-node/register": "^1.10.10",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.11.13",
        "@swc/helpers": "^0.5.15",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "chalk": "^5.4.1",
        "chokidar": "^4.0.3",
        "commander": "^11.1.0",
        "dotenv": "^16.4.7",
        "enquirer": "^2.4.1",
        "esbuild": "^0.25.2",
        "esbuild-register": "^3.6.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-prettier": "^5.2.5",
        "execa": "^9.5.2",
        "fs-extra": "^11.3.0",
        "husky": "^9.1.7",
        "inquirer": "^9.3.7",
        "lint-staged": "^15.5.0",
        "npm-run-path": "5.3.0",
        "ora": "^6.3.1",
        "prettier": "^3.5.3",
        "prompts": "^2.4.2",
        "semver": "^7.7.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tslib": "^2.8.1",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vitest": "^3.0.9",
        "yargs": "^17.7.2"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^20.17.28",
        "conventional-changelog": "^6.0.0",
        "conventional-changelog-cli": "^5.0.0"
    },
    "lint-staged": {
        "**/*.{ts,json}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@swc/core",
            "esbuild"
        ]
    }
}
